/*
 * -------------------------------------------------
 *  nfcore custom profile Nextflow config file
 * -------------------------------------------------
 * Config options for all custom environments.
 * Cluster-specific config options should be saved
 * in the conf folder and imported under a profile
 * name here.
 */

//Please use a new line per include Config section to allow easier linting/parsing. Thank you.
profiles {
  awsbatch     { includeConfig "${params.custom_config_base}/conf/empress_awsbatch.config" }
  empress      { includeConfig "${params.custom_config_base}/conf/empress_tower.config" }
  emp_tower_prod { includeConfig "${params.custom_config_base}/conf/empress_tower_prod.config" } 
  emp_tower_sandbox { includeConfig "${params.custom_config_base}/conf/empress_tower_sandbox.config" } 
  }
